knitr::opts_chunk$set(echo = F , message = F , warning = F)
library(openxlsx)
library(tidyverse)
library(DT)
rm(list = ls())
p1 <- read.xlsx("Liste_Partitions.xlsx") %>%
arrange(titre) %>%
mutate(fichier = paste0('<a href="sheets/' ,
fichier ,
'"  target="_blank">' ,
'Lien</a>')) %>%
mutate(video = ifelse(!is.na(video) ,
paste0('<a href="videos/' ,
video ,
'"  target="_blank">' ,
'Lien</a>') ,
NA))
knitr::opts_chunk$set(echo = F , message = F , warning = F)
library(htmltools)
View(p1)
